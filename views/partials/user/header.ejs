<header class="header header-6">
  <div class="header-middle">
    <div class="container">
      <div class="header-left">
        <div class="header-search header-search-extended header-search-visible d-none d-lg-block">
          <a href="#" class="search-toggle" role="button"><i class="icon-search"></i></a>
          <form action="#" method="get">
            <div class="header-search-wrapper search-wrapper-wide">
              <label for="q" class="sr-only">Search</label>
              <button class="btn btn-primary" type="submit"><i class="icon-search"></i></button>
              <input type="search" class="form-control" name="q" id="q" placeholder="Search product ..." required>
            </div><!-- End .header-search-wrapper -->
          </form>
        </div><!-- End .header-search -->
      </div>
      <div class="header-center">
        <a href="" class="logo">
          <img src="assets/images/shutter.png" alt="Molla Logo" width="125" height="70">
        </a>
      </div><!-- End .header-left -->



      <div class="header-right">

        <% if (typeof user !== 'undefined') { %>
        <% if (user) { %>
        <div class="nav-item navbar-icon-link">
          <a href="/user/profile" class="navbar-icon-link">
            <i class="bi bi-person text-success"></i>
            <span class="text-sm ms-2 ms-lg-0 text-uppercase text-sm fw-bold d-none d-sm-inline d-lg-none">
              Account
            </span>
          </a>
        </div>
        <% } %>
        <% } else { %>
       
            <a href="/login" title="login">
              <div class="icon">
                <i class="icon-user"></i>
              </div>
    
            </a>
        <% } %>

        <% if (typeof user !=='undefined') { %>
          <% if (user) { %>
          <div class="nav-item navbar-icon-link m-0">
            <a href="/logout" class="navbar-icon-link text-danger px-2 py-1 align-items-center">
              <i class="bi bi-box-arrow-right align-items-center"></i>
              <span class="text-sm ms-2 ms-lg-0 text-uppercase text-sm fw-bold d-none d-sm-inline d-lg-none">
                Sign Out
              </span>
            </a>
          </div>
          <% } %>
          <% } %>

        <a href="wishlist.html" class="wishlist-link">
          <i class="icon-heart-o"></i>

        </a>

        <div class="dropdown cart-dropdown">
          <a href="#" class="dropdown-toggle" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" data-display="static">
            <i class="icon-shopping-cart"></i>

          </a>
        </div><!-- End .cart-dropdown -->
      </div>
    </div><!-- End .container -->
  </div><!-- End .header-middle -->

  <div class="header-bottom sticky-header">
    <div class="container">
      <div class="header-center">
        <nav class="main-nav ">
          <ul class="menu">

            <li class="megamenu-container active">
              <a href="" class="">Home</a>

            </li>
            <li>
              <a href="" class="">Shop</a>

            </li>
            <li>
              <a href="" class="">About</a>


            </li>
            <li>
              <a href="" class="">Contact</a>


            </li>



          </ul><!-- End .menu -->
        </nav><!-- End .main-nav -->

        <button class="mobile-menu-toggler">
          <span class="sr-only">Toggle mobile menu</span>
          <i class="icon-bars"></i>
        </button>
      </div><!-- End .header-left -->

    </div><!-- End .container -->
  </div><!-- End .header-bottom -->
</header><!-- End .header -->
